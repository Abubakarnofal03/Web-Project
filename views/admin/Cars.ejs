        const logout = document.getElementById("landingpage");
        const sparepartspage = document.getElementById("sparepartspage");
        const carspage = document.getElementById("carspage");
        const bikespage = document.getElementById("bikespage");
        const homepage = document.getElementById("adminhome");
        const userpage = document.getElementById("userpage");
        const feedbackpage = document.getElementById("feedbackpage");

        homepage.addEventListener("click", () => {
            location.href = "/admin/adminhome";
        })
        sparepartspage.addEventListener("click", () => {
            location.href = "/admin/Spareparts";
        })
        
        bikespage.addEventListener("click", () =>{
             location.href ="/admin/Bikes";
        })

        carspage.addEventListener("click", () =>{
             location.href ="/admin/Cars";
        })

        userpage.addEventListener("click", () =>{
             location.href ="/admin/Users";
        })

        feedbackpage.addEventListener("click", () =>{
             location.href ="/admin/Feedbacks";
        })



        const updatecars = document.getElementById("updatecarspage");
        const updatebikes = document.getElementById("updatesparepartspage");
        const updatespareparts = document.getElementById("updatebikespage");
        const updateusers = document.getElementById("updateuserspage");

        const deleteecars = document.getElementById("deletecarspage");
        const deletebikes = document.getElementById("deletesparepartspage");
        const deletespareparts = document.getElementById("deletebikespage");
        const deleteeusers = document.getElementById("deleteuserspage");

        const addcars = document.getElementById("addcarspage");
        const addbikes = document.getElementById("addsparepartspage");
        const addspareparts = document.getElementById("addbikespage");
        const adduser = document.getElementById("adduserspage");


        updatebikes.addEventListener("click", () => {
            location.href = "/admin/updatebikes";
        })
        updatespareparts.addEventListener("click", () => {
            location.href = "/admin/updatespareparts";
        })
        updatecars.addEventListener("click", () =>{
             location.href ="/admin/updatecars";
        })
        updateusers.addEventListener("click", () =>{
             location.href ="/admin/updateusers";
        })


        addbikes.addEventListener("click", () => {
            location.href = "/admin/addbikes";
        })
        addspareparts.addEventListener("click", () => {
            location.href = "/admin/addspareparts";
        })
        addcars.addEventListener("click", () =>{
             location.href ="/admin/addcars";
        })
        adduser.addEventListener("click", () =>{
             location.href ="/admin/addusers";
        })


        deletebikes.addEventListener("click", () => {
            location.href = "/admin/deletebikes";
        })
        deletespareparts.addEventListener("click", () => {
            location.href = "/admin/deletespareparts";
        })
        deleteecars.addEventListener("click", () =>{
             location.href ="/admin/deletecars";
        })
        deleteeusers.addEventListener("click", () =>{
             location.href ="/admin/deleteusers";
        })

        

        

    </script>
