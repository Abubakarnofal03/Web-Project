<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Automotive Studio</title>
    <link rel="icon" type="image/x-icon" href="/Images/favicon.ico">
    <link rel="stylesheet" href="/css/style.css">
    <link href='https://fonts.googleapis.com/css?family=Cairo' rel='stylesheet'>
    <link href='https://fonts.googleapis.com/css?family=Aclonica' rel='stylesheet'>
    <link href='https://fonts.googleapis.com/css?family=Caveat' rel='stylesheet'>
    <link href='https://fonts.googleapis.com/css?family=Bai Jamjuree' rel='stylesheet'>
</head>

<body>
    <header>
        <div class="headerclass">

            <ul class="navbar">
              <!--<li><a href="/ContactUs">CONTACT US</a></li> -->
               <!--<li><a href="/AboutUs">ABOUT US</a></li> -->
               <li><a href="/landing" id="landingpage">LOG OUT</a></li>
                <li><a href="/admin/Feedbacks" id="feedbackpage">FEEDBACK</a></li>
                <li><a href="/admin/Users" id="userpage">USERS</a></li>
                <li><a href="/admin/Spareparts" id="sparepartspage">SPARE PARTS</a></li>
                <li><a href="/admin/Bikes"  id="bikespage">BIKES</a></li>
                <li><a href="/admin/Cars" id="carspage">CARS</a></li>
                <li><a href="/admin/adminhome" id ="adminhome">HOME</a></li>

               <li class="titlehome">AUTOMOTIVE STUDIO</li>
            </ul>
        </div>
    </header>
    <!-- body section  -->
     <div>
        <div>
            <div class="leftcenter">
                <img src="/Images/logo.png" alt="logo">
            </div>
            <h2>Find A  Best Car To Purchase</h2>
         </div>
     </div>

     <!-- display products -->
     <br>
     <h3>CAR DETAILS</h3>
     <br>
     <div class="displaycenter">
        <table>
            <thead>
                <tr>
                    <TH>ID</TH>
                    <TH>IMAGE</TH>
                    <TH>CAR NAME</TH>
                    <TH>COMPNAY NAME</TH>
                    <TH>MODEL</TH>
                    <TH>PRICE</TH>
                    <TH>PHONE NUMBER</TH>
                </tr>
            </thead>
            
            <tbody>
                <% Carsdata.forEach(function(Car) { %>
                    <tr>
                        <% console.log(Car.imagePath); %>
                        <% console.log(Car.imageHERE); %>
                        <td class="productid"><%= Car.id %></td>
                        <td class="productblocks"><img class="displayimg" src="<%= Car.picture %>" alt="gaddi_di_tasver"></td>
                        <td class="productblocks"><%= Car.carName %></td>
                        <td class="productblocks"><%= Car.companyName %></td>
                        <td class="productblocks"><%= Car.model %></td>
                        <td class="productblocks"><%= Car.price %></td>
                        <td class="productblocks"><%= Car.phonenumber %></td>
                    </tr>
                    <% }); %>
            </tbody>
            
          </table>
     </div>
     
     <div class="center centerbox2">
        <a class="pages" href="/admin/Cars" name="0">1</a>
        <a class="pages" href="/admin/Cars" name="1">2</a>
        <a class="pages" href="/admin/Cars" name="2">3</a>
        <a class="pages" href="/admin/Cars" name="3">4</a>
        <a class="pages" href="/admin/Cars" name="4">5</a>...
        <a class="pages" href="/admin/Cars" name="5">Next</a>
    </div>

    <!--end body section  -->
    <footer>
        <p>Â© All Rights Reserved. Design by <a href="#"> Tahir </a></p>
    </footer>


